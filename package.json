{
    "name": "dauntless-builder",
    "version": "2.0.0",
    "type": "module",
    "repository": "git@github.com:atomicptr/dauntless-builder",
    "author": "Christopher Kaster <me@atomicptr.de>",
    "license": "AGPLv3",
    "private": true,
    "scripts": {
        "build": "npm run build:deps && npm run build:frontend",
        "build:deps": "tsx --tsconfig ./builder/tsconfig.json ./builder/src/main.ts",
        "build:data": "npm run build:deps -- --run data i18n build-finder-intermediate-data",
        "build:frontend": "vite build",
        "dev": "vite dev",
        "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
        "bench": "npm run benchmark",
        "benchmark": "npm run test -- --projects jest-benchmark.config.ts",
        "deploy": "npm run build && ./scripts/deploy.sh",
        "lint": "concurrently 'npm:lint:*(!fix)'",
        "lint:tsc": "tsc",
        "lint:code": "eslint .",
        "lint:json": "scripts/json-linter -r src tsconfig.json .eslintrc",
        "lint:fix": "concurrently npm:lint:fix:*",
        "lint:fix:json": "npm run lint:json -- --fix",
        "lint:fix:code": "prettier . --write && eslint . --fix"
    },
    "engines": {
        "node": ">=18.0.0",
        "yarn": "please-use-npm"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.19",
        "@mui/material": "^5.14.19",
        "@tanstack/react-query": "^5.12.2",
        "hashids": "^2.3.0",
        "i18next": "^23",
        "i18next-browser-languagedetector": "^7.2.0",
        "i18next-resources-to-backend": "^1.2.0",
        "jotai": "^2.6.0",
        "just-kebab-case": "^4.2.0",
        "md5": "^2.3.0",
        "notistack": "^3.0.1",
        "react": "^18.2.0",
        "react-country-flag": "^3.1.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^4.0.11",
        "react-helmet": "^6.1.0",
        "react-i18next": "^13.5.0",
        "react-icons": "^4.12.0",
        "react-lazy-load-image-component": "^1.6.0",
        "react-router-dom": "^6.20.1",
        "react-virtualized-auto-sizer": "^1.0.20",
        "react-window": "^1.8.10",
        "sqids": "^0.3.0",
        "stylis": "^4.3.0",
        "stylis-plugin-rtl": "^2.1.1",
        "ts-pattern": "^5.0.5"
    },
    "devDependencies": {
        "@crowdin/crowdin-api-client": "^1.28.1",
        "@types/command-line-args": "^5.2.3",
        "@types/glob": "^8.1.0",
        "@types/jest": "^29.5.10",
        "@types/js-yaml": "^4.0.9",
        "@types/md5": "^2.3.5",
        "@types/node": "^20.10.2",
        "@types/react": "^18.2.41",
        "@types/react-dom": "^18.2.17",
        "@types/react-helmet": "^6.1.9",
        "@types/react-lazy-load-image-component": "^1.6.3",
        "@types/react-virtualized-auto-sizer": "^1.0.3",
        "@types/react-window": "^1.8.8",
        "@types/sort-json": "^2.0.3",
        "@types/stylis": "^4.2.4",
        "@types/yauzl": "^2.10.3",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "@vitejs/plugin-react": "^4.2.0",
        "ajv": "^8.12.0",
        "axios": "^1.6.2",
        "command-line-args": "^5.2.1",
        "concurrently": "^8.2.2",
        "eslint": "^8.55.0",
        "eslint-plugin-i18next": "^6.0.3",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-unused-imports": "^3.0.0",
        "glob": "^10.3.10",
        "jest": "^29",
        "jest-bench": "^29",
        "jest-environment-jsdom": "^29.7.0",
        "js-yaml": "^4.1.0",
        "license-checker": "^25.0.1",
        "prettier": "^3.1.0",
        "queue-promise": "^2.2.1",
        "sort-json": "^2.0.1",
        "ts-jest": "^29",
        "tsx": "^4.6.2",
        "typescript": "^5.3.2",
        "vite": "^5.0.4",
        "vite-plugin-favicon": "^1.0.8",
        "vite-plugin-pwa": "^0.17.2",
        "yauzl": "^2.10.0"
    }
}
